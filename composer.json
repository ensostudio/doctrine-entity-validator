  "require": {
    "php": ">=8.1",
    "doctrine/orm": "^3.3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.22"
  },
  "autoload": {
    "psr-4": {
      "EnsoStudio\\Doctrine\\ORM\\": "src/"
